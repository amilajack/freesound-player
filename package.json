{
  "name": "freesound-player",
  "version": "1.0.0",
  "description": "freesound player",
  "license": "MIT",
  "homepage": "https://amilajack.github.io/freesound-player",
  "keywords": [
    "react",
    "starter"
  ],
  "main": "src/index.js",
  "dependencies": {
    "@colormap/core": "0.2.0",
    "@colormap/presets": "0.2.0",
    "freesound-client": "^0.2.0-2",
    "lodash": "4.17.20",
    "peaks.js": "0.21.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "react-waveform": "0.1.0",
    "react-wavy-audio": "1.0.3",
    "sample-editor-view": "1.0.0",
    "wavesurfer.js": "4.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.8",
    "eslint-config-bliss": "^5.0.0",
    "eslint-plugin-react": "^7.20.6",
    "gh-pages": "^3.1.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "deploy": "yarn build && gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "renovate": {
    "extends": [
      "bliss"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "bliss",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/prop-types": "off"
    }
  }
}
